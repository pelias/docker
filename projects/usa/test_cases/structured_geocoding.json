      "id": 1,
      "status": "pass",
      "user": "trescube",
      "type": "dev",
      "notes": "full address found should return that result",
      "in": {
        "address": "1090 N Charlotte Street",
        "locality": "Lancaster",
        "region": "PA"
      },
      "expected": {
        "properties": [
          {
            "layer": "address",
            "name": "1090 N Charlotte Street",
            "country_a": "USA",
            "country": "United States",
            "region": "Pennsylvania",
            "region_a": "PA",
            "county": "Lancaster County",
            "locality": "Lancaster",
            "housenumber": "1090",
            "street": "N Charlotte Street"
